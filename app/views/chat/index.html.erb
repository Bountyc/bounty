<%= stylesheet_link_tag    'chat', media: 'all', 'data-turbolinks-track' => true %>

<%= react_component("Chat", {messages: @messages, loadUrl: chat_index_path(user: @chat_partner.id, :format => :json), pollInterval: 2000, user_id: current_user.id, user: current_user.first_name + " " + current_user.last_name, chatWithUser: @chat_partner.first_name + " " + @chat_partner.last_name, form: @react_form}) %>